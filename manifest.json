{
     "manifest_version": 2,
     "name": "Web WhatsApp",
     "version": "1",
     "icons": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
     },
     "permissions": [
          "webview",
          "notifications",
          "audio",
          "audioCapture",
          "videoCapture"
     ],
     "app": {
          "background": {
               "scripts": ["background.js"]
          }
     },
     "minimum_chrome_version": "28"
}
